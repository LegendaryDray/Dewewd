local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/joeengo/exploiting/main/EngoUILIB_V2.lua", true))()

local main = library:CreateMain("Credits", "Main","Universal","Troll" Enum.KeyCode.LeftAlt)

local tab = main:CreateTab("Credits")

creditsTab.newLabel("🌟 Hub Creator: uniqve")
creditsTab.newLabel("🧪 Script Tester: qweiringss")
creditsTab.newLabel("🎨 UI Design: DrRay Library")
creditsTab.newLabel("🙏 Special Thanks: AZYsGithub")
creditsTab.newLabel("🔄 Version: 5.0 Updated")
creditsTab.newLabel("🌐 Additional Credits: External Z Hub Community")

local tab = main:CreateTab("Main")

mainTab.newLabel("📝 Credits: External Team")
mainTab.newLabel("🔄 Updated")
mainTab.newLabel("🔍 Find more stuff in the last tab")
mainTab.newLabel("🔧 Useful stuff")

-- Tools in Main Tab
mainTab.newButton("🔄 Rejoin", "Yes sir, it's self-explanatory", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
end)

mainTab.newButton("🔄 Serverhop", "Joins you in a different server", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/VduBT2nt"))()
end)

mainTab.newButton("🔍 Script searcher", "Useful if you don't have an executor with global scripts", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/chillz-workshop/main/ScriptSearcher"))()
end)

mainTab.newButton("⌨️ Keyboard", "Keyboard for mobile", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end)

mainTab.newButton("🛠️ Dex explorer", "Dex for mobile to steal game scripts and delete stuff", function()
    getgenv().Key = "Bash"
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/Refinement/main/MC%3AIY%20Dex", true))()
end)

tab:CreateLabel("Universal")

mainTab.newButton("🦘 Inf Jump", "Gives you infinite jump so you can jump forever", function()
    local InfiniteJumpEnabled = true
    game:GetService("UserInputService").JumpRequest:connect(function()
        if InfiniteJumpEnabled then
            game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
        end
    end)
end)

mainTab.newButton("👀 Chat spy [FE]", "Spy on private chat", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Chat-Spy-Script-2104"))()
end)

mainTab.newButton("🚪 Noclip", "Walk through walls; press 'E' to toggle", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/yxvrgmMH"))()
end)

mainTab.newButton("✈️ Fly GUI", "A fly GUI to make you fly", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/zXUSuxbs"))()
end)

local tab = main:CreateTab("Troll")

trollTab.newButton("⚡ FPS Booster", "Improves game performance", function()
    setfpscap(1000)
end)

trollTab.newButton("⏲️ Anti-AFK", "Prevents being kicked", function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)

